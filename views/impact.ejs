<div class="min-h-screen bg-gray-50 ">
    <div class="max-w-7xl mx-auto py-6 sm:py-8 px-4 sm:px-6 lg:px-8">

        <!-- Header Section -->
        <div class="mb-8">
            <div class="flex flex-col lg:flex-row lg:items-center lg:justify-between gap-6">
                <!-- Title -->
                <div>
                    <h1 class="text-3xl sm:text-4xl font-bold text-gray-900 ">
                        Program Usage Analytics
                    </h1>
                    <p class="mt-2 text-base text-gray-600 ">
                        Tracking usage metrics and digital navigator performance
                    </p>

                    <!-- Tabs -->
                    <div class="mt-6 border-b border-gray-200 ">
                        <nav class="-mb-px flex space-x-8" aria-label="Tabs">
                            <a href="/"
                                class="border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm">
                                General Usage
                            </a>
                            <a href="/agents"
                                class="border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm">
                                By Agent
                            </a>
                            <a href="/impact"
                                class="border-blue-500 text-blue-600 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm"
                                aria-current="page">
                                Impact
                            </a>
                        </nav>
                    </div>
                </div>

                <!-- Filter Section -->
                <div class="bg-white rounded-xl shadow-sm border border-gray-200 p-4">
                    <form action="/impact" method="GET" class="flex flex-col sm:flex-row gap-3">
                        <!-- Date Range -->
                        <div class="flex gap-2 items-end">
                            <div>
                                <label for="start_date" class="block text-xs font-medium text-gray-700 mb-1">
                                    Start Date
                                </label>
                                <input type="date" name="start_date" id="start_date" value="<%= startDate %>"
                                    class="block w-full rounded-lg border-gray-300 shadow-sm focus:border-blue-500 focus:ring-blue-500 text-sm">
                            </div>
                            <div>
                                <label for="end_date" class="block text-xs font-medium text-gray-700 mb-1">
                                    End Date
                                </label>
                                <input type="date" name="end_date" id="end_date" value="<%= endDate %>"
                                    class="block w-full rounded-lg border-gray-300 shadow-sm focus:border-blue-500 focus:ring-blue-500 text-sm">
                            </div>
                        </div>

                        <!-- Buttons -->
                        <div class="flex gap-2 items-end">
                            <button type="submit"
                                class="inline-flex items-center justify-center px-4 py-2 rounded-lg border border-transparent bg-blue-600 text-sm font-medium text-white shadow-sm hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 transition-colors">
                                <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M3 4a1 1 0 011-1h16a1 1 0 011 1v2.586a1 1 0 01-.293.707l-6.414 6.414a1 1 0 00-.293.707V17l-4 4v-6.586a1 1 0 00-.293-.707L3.293 7.293A1 1 0 013 6.586V4z" />
                                </svg>
                                Filter
                            </button>
                            <button type="button" onclick="window.location.reload()"
                                class="inline-flex items-center justify-center px-4 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 shadow-sm hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2 transition-colors">
                                <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15" />
                                </svg>
                                Refresh
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

        <% if (!zendeskConfigured) { %>
        <!-- Zendesk Not Configured Warning -->
        <div class="mb-8 bg-amber-50 border border-amber-200 rounded-xl p-4">
            <div class="flex items-center">
                <svg class="w-5 h-5 text-amber-500 mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                        d="M12 9v2m0 4h.01m-6.938 4h13.856c1.54 0 2.502-1.667 1.732-2.5L13.732 4c-.77-.833-1.964-.833-2.732 0L4.082 16.5c-.77.833.192 2.5 1.732 2.5z" />
                </svg>
                <div>
                    <p class="text-sm font-medium text-amber-800">Zendesk not configured</p>
                    <p class="text-xs text-amber-700 mt-1">Set <code class="bg-amber-100 px-1 rounded">ZENDESK_SUBDOMAIN</code>, <code class="bg-amber-100 px-1 rounded">ZENDESK_EMAIL</code>, and <code class="bg-amber-100 px-1 rounded">ZENDESK_API_TOKEN</code> environment variables to enable ticket matching.</p>
                </div>
            </div>
        </div>
        <% } %>

        <!-- Summary Metrics -->
        <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-6 mb-8">
            <!-- Total Searches -->
            <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden hover:shadow-md transition-shadow">
                <div class="p-6">
                    <div class="flex items-center">
                        <div class="flex-shrink-0">
                            <div class="flex items-center justify-center w-12 h-12 rounded-lg bg-blue-100">
                                <svg class="w-6 h-6 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M21 21l-6-6m2-5a7 7 0 11-14 0 7 7 0 0114 0z" />
                                </svg>
                            </div>
                        </div>
                        <div class="ml-4 flex-1">
                            <dt class="text-sm font-medium text-gray-500">Total Searches</dt>
                            <dd class="mt-1 text-2xl font-bold text-gray-900"><%= Number(totalSearches).toLocaleString() %></dd>
                        </div>
                    </div>
                </div>
                <div class="bg-blue-50 px-6 py-2">
                    <div class="text-xs text-blue-700 font-medium"><%= startDate %> - <%= endDate %></div>
                </div>
            </div>

            <!-- Unique Addresses -->
            <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden hover:shadow-md transition-shadow">
                <div class="p-6">
                    <div class="flex items-center">
                        <div class="flex-shrink-0">
                            <div class="flex items-center justify-center w-12 h-12 rounded-lg bg-yellow-100">
                                <svg class="w-6 h-6 text-yellow-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                                </svg>
                            </div>
                        </div>
                        <div class="ml-4 flex-1">
                            <dt class="text-sm font-medium text-gray-500">Unique Addresses</dt>
                            <dd class="mt-1 text-2xl font-bold text-gray-900"><%= Number(uniqueAddresses).toLocaleString() %></dd>
                        </div>
                    </div>
                </div>
                <div class="bg-yellow-50 px-6 py-2">
                    <div class="text-xs text-yellow-700 font-medium">Distinct locations searched</div>
                </div>
            </div>

            <!-- Sales -->
            <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden hover:shadow-md transition-shadow">
                <div class="p-6">
                    <div class="flex items-center">
                        <div class="flex-shrink-0">
                            <div class="flex items-center justify-center w-12 h-12 rounded-lg bg-emerald-100">
                                <svg class="w-6 h-6 text-emerald-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
                                </svg>
                            </div>
                        </div>
                        <div class="ml-4 flex-1">
                            <dt class="text-sm font-medium text-gray-500">Sales</dt>
                            <dd class="mt-1 text-2xl font-bold text-emerald-600"><%= totalWithInternet %></dd>
                        </div>
                    </div>
                </div>
                <div class="bg-emerald-50 px-6 py-2">
                    <div class="text-xs text-emerald-700 font-medium">Successful resolutions from searches</div>
                </div>
            </div>

            <!-- Conversion Rate -->
            <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden hover:shadow-md transition-shadow">
                <div class="p-6">
                    <div class="flex items-center">
                        <div class="flex-shrink-0">
                            <div class="flex items-center justify-center w-12 h-12 rounded-lg bg-purple-100">
                                <svg class="w-6 h-6 text-purple-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                        d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" />
                                </svg>
                            </div>
                        </div>
                        <div class="ml-4 flex-1">
                            <dt class="text-sm font-medium text-gray-500">Conversion Rate</dt>
                            <dd class="mt-1 text-2xl font-bold text-purple-600"><%= conversionRate %>%</dd>
                        </div>
                    </div>
                </div>
                <div class="bg-purple-50 px-6 py-2">
                    <div class="text-xs text-purple-700 font-medium">Search to internet signup</div>
                </div>
            </div>
        </div>

        <!-- Impact Table -->
        <div class="mb-8">
            <div class="bg-white rounded-xl shadow-sm border border-gray-200 overflow-hidden">
                <!-- Table Header with Ticket Filter -->
                <div class="px-6 py-5 border-b border-gray-200 flex flex-col sm:flex-row sm:items-center sm:justify-between gap-4">
                    <div>
                        <h2 class="text-xl font-semibold text-gray-900 flex items-center">
                            <svg class="w-6 h-6 mr-3 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                    d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01" />
                            </svg>
                            Address Impact Tracking
                        </h2>
                        <p class="mt-1 text-sm text-gray-500">Showing <%= totalFiltered %> address<%= totalFiltered !== 1 ? 'es' : '' %> (page <%= currentPage %> of <%= totalPages %>)</p>
                    </div>

                    <!-- Ticket Filter Tabs -->
                    <div class="flex rounded-lg border border-gray-200 overflow-hidden">
                        <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=internet"
                            class="px-4 py-2 text-sm font-medium <%= ticketFilter === 'internet' ? 'bg-emerald-600 text-white' : 'bg-white text-gray-700 hover:bg-gray-50' %> border-r border-gray-200 transition-colors">
                            Sales (<%= totalWithInternet %>)
                        </a>
                        <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=no_ticket"
                            class="px-4 py-2 text-sm font-medium <%= ticketFilter === 'no_ticket' ? 'bg-orange-600 text-white' : 'bg-white text-gray-700 hover:bg-gray-50' %> border-r border-gray-200 transition-colors">
                            No Ticket (<%= totalWithoutTickets %>)
                        </a>
                        <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=all"
                            class="px-4 py-2 text-sm font-medium <%= ticketFilter === 'all' ? 'bg-blue-600 text-white' : 'bg-white text-gray-700 hover:bg-gray-50' %> transition-colors">
                            All (<%= uniqueAddresses %>)
                        </a>
                    </div>
                </div>

                <div class="overflow-x-auto">
                    <table class="min-w-full divide-y divide-gray-200">
                        <thead class="bg-gray-50">
                            <tr>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    Address
                                </th>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    Searches
                                </th>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    Last Searched
                                </th>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    Zendesk Status
                                </th>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    DN Resolution
                                </th>
                                <th scope="col" class="px-6 py-4 text-left text-xs font-semibold text-gray-600 uppercase tracking-wider">
                                    Ticket Details
                                </th>
                            </tr>
                        </thead>
                        <tbody class="bg-white divide-y divide-gray-200">
                            <% if (addresses.length > 0) { %>
                                <% addresses.forEach(addr => { %>
                                <tr class="hover:bg-gray-50 transition-colors">
                                    <td class="px-6 py-4">
                                        <div class="text-sm font-medium text-gray-900 max-w-sm truncate" title="<%= addr.address %>">
                                            <%= addr.address %>
                                        </div>
                                    </td>
                                    <td class="px-6 py-4 whitespace-nowrap">
                                        <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-blue-100 text-blue-800">
                                            <%= addr.searchCount %>
                                        </span>
                                    </td>
                                    <td class="px-6 py-4 whitespace-nowrap">
                                        <div class="text-sm text-gray-600"><%= addr.lastSearched %></div>
                                    </td>
                                    <td class="px-6 py-4 whitespace-nowrap">
                                        <% if (addr.zendeskError && !addr.hasTicket) { %>
                                            <% if (addr.zendeskError === 'Zendesk credentials not configured') { %>
                                                <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-gray-100 text-gray-600">
                                                    Not configured
                                                </span>
                                            <% } else { %>
                                                <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-red-100 text-red-700" title="<%= addr.zendeskError %>">
                                                    Error
                                                </span>
                                            <% } %>
                                        <% } else if (addr.hasTicket) { %>
                                            <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">
                                                <svg class="w-3 h-3 mr-1" fill="currentColor" viewBox="0 0 20 20">
                                                    <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd" />
                                                </svg>
                                                <%= addr.ticketCount %> ticket<%= addr.ticketCount > 1 ? 's' : '' %>
                                            </span>
                                        <% } else { %>
                                            <span class="inline-flex items-center px-2.5 py-1 rounded-full text-xs font-medium bg-orange-100 text-orange-700">
                                                No ticket
                                            </span>
                                        <% } %>
                                    </td>
                                    <td class="px-6 py-4">
                                        <% if (addr.hasTicket && addr.tickets.length > 0) { %>
                                            <%
                                                const resolutions = addr.tickets
                                                    .filter(t => t.dnResolution)
                                                    .map(t => t.dnResolution.replace(/_/g, ' ').replace(/\d+$/, '').trim());
                                                const uniqueResolutions = [...new Set(resolutions)];
                                            %>
                                            <% if (uniqueResolutions.length > 0) { %>
                                                <div class="flex flex-wrap gap-1">
                                                    <% uniqueResolutions.forEach(r => { %>
                                                        <span class="inline-flex items-center px-2 py-0.5 rounded text-[11px] font-medium
                                                            <%= r.includes('internet') || r.includes('signed up for internet') ? 'bg-emerald-100 text-emerald-800' :
                                                               r.includes('computer') || r.includes('purchased') ? 'bg-blue-100 text-blue-800' :
                                                               r.includes('digital') || r.includes('enroll') ? 'bg-purple-100 text-purple-800' :
                                                               r.includes('tech support') || r.includes('referred') ? 'bg-yellow-100 text-yellow-800' :
                                                               'bg-gray-100 text-gray-700' %>">
                                                            <%= r %>
                                                        </span>
                                                    <% }); %>
                                                </div>
                                            <% } else { %>
                                                <span class="text-xs text-gray-400">-</span>
                                            <% } %>
                                        <% } else { %>
                                            <span class="text-xs text-gray-400">-</span>
                                        <% } %>
                                    </td>
                                    <td class="px-6 py-4">
                                        <% if (addr.hasTicket && addr.tickets.length > 0) { %>
                                            <div class="space-y-1">
                                                <% addr.tickets.slice(0, 3).forEach(ticket => { %>
                                                    <div class="text-xs">
                                                        <% if (zendeskConfigured) { %>
                                                            <a href="https://<%= process.env.ZENDESK_SUBDOMAIN %>.zendesk.com/agent/tickets/<%= ticket.id %>"
                                                               target="_blank" rel="noopener"
                                                               class="text-blue-600 hover:text-blue-800 font-medium">
                                                                #<%= ticket.id %>
                                                            </a>
                                                        <% } else { %>
                                                            <span class="font-medium text-gray-700">#<%= ticket.id %></span>
                                                        <% } %>
                                                        <span class="text-gray-500 ml-1"><%= ticket.subject %></span>
                                                        <span class="inline-flex items-center px-1.5 py-0.5 rounded text-[10px] font-medium ml-1
                                                            <%= ticket.status === 'open' ? 'bg-red-100 text-red-700' :
                                                               ticket.status === 'pending' ? 'bg-yellow-100 text-yellow-700' :
                                                               ticket.status === 'solved' ? 'bg-green-100 text-green-700' :
                                                               ticket.status === 'closed' ? 'bg-gray-100 text-gray-600' :
                                                               'bg-blue-100 text-blue-700' %>">
                                                            <%= ticket.status %>
                                                        </span>
                                                    </div>
                                                <% }); %>
                                                <% if (addr.tickets.length > 3) { %>
                                                    <div class="text-xs text-gray-400">+ <%= addr.tickets.length - 3 %> more</div>
                                                <% } %>
                                            </div>
                                        <% } else { %>
                                            <span class="text-xs text-gray-400">-</span>
                                        <% } %>
                                    </td>
                                </tr>
                                <% }); %>
                            <% } else { %>
                                <tr>
                                    <td colspan="6" class="px-6 py-12 text-center">
                                        <div class="flex flex-col items-center justify-center text-gray-500">
                                            <svg class="w-12 h-12 mb-4 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                                    d="M20 13V6a2 2 0 00-2-2H6a2 2 0 00-2 2v7m16 0v5a2 2 0 01-2 2H6a2 2 0 01-2-2v-5m16 0h-2.586a1 1 0 00-.707.293l-2.414 2.414a1 1 0 01-.707.293h-3.172a1 1 0 01-.707-.293l-2.414-2.414A1 1 0 006.586 13H4" />
                                            </svg>
                                            <p class="text-sm font-medium">No addresses found for this filter</p>
                                            <p class="text-xs mt-1">Try changing the filter or adjusting your date range</p>
                                        </div>
                                    </td>
                                </tr>
                            <% } %>
                        </tbody>
                    </table>
                </div>

                <% if (totalPages > 1) { %>
                <!-- Pagination -->
                <div class="px-6 py-4 border-t border-gray-200 flex items-center justify-between">
                    <div class="text-sm text-gray-500">
                        Showing <%= ((currentPage - 1) * perPage) + 1 %> - <%= Math.min(currentPage * perPage, totalFiltered) %> of <%= totalFiltered %> addresses
                    </div>
                    <nav class="flex items-center gap-1" aria-label="Pagination">
                        <% if (currentPage > 1) { %>
                            <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=<%= ticketFilter %>&page=<%= currentPage - 1 %>"
                                class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors">
                                <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
                                </svg>
                                Prev
                            </a>
                        <% } else { %>
                            <span class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-200 bg-gray-50 text-sm font-medium text-gray-400 cursor-not-allowed">
                                <svg class="w-4 h-4 mr-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 19l-7-7 7-7" />
                                </svg>
                                Prev
                            </span>
                        <% } %>

                        <%
                            let startPage = Math.max(1, currentPage - 2);
                            let endPage = Math.min(totalPages, startPage + 4);
                            if (endPage - startPage < 4) startPage = Math.max(1, endPage - 4);
                        %>

                        <% if (startPage > 1) { %>
                            <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=<%= ticketFilter %>&page=1"
                                class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors">
                                1
                            </a>
                            <% if (startPage > 2) { %>
                                <span class="px-2 text-gray-400">...</span>
                            <% } %>
                        <% } %>

                        <% for (let p = startPage; p <= endPage; p++) { %>
                            <% if (p === currentPage) { %>
                                <span class="inline-flex items-center px-3 py-2 rounded-lg border border-blue-500 bg-blue-600 text-sm font-medium text-white">
                                    <%= p %>
                                </span>
                            <% } else { %>
                                <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=<%= ticketFilter %>&page=<%= p %>"
                                    class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors">
                                    <%= p %>
                                </a>
                            <% } %>
                        <% } %>

                        <% if (endPage < totalPages) { %>
                            <% if (endPage < totalPages - 1) { %>
                                <span class="px-2 text-gray-400">...</span>
                            <% } %>
                            <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=<%= ticketFilter %>&page=<%= totalPages %>"
                                class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors">
                                <%= totalPages %>
                            </a>
                        <% } %>

                        <% if (currentPage < totalPages) { %>
                            <a href="/impact?start_date=<%= startDate %>&end_date=<%= endDate %>&ticket_filter=<%= ticketFilter %>&page=<%= currentPage + 1 %>"
                                class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-300 bg-white text-sm font-medium text-gray-700 hover:bg-gray-50 transition-colors">
                                Next
                                <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                                </svg>
                            </a>
                        <% } else { %>
                            <span class="inline-flex items-center px-3 py-2 rounded-lg border border-gray-200 bg-gray-50 text-sm font-medium text-gray-400 cursor-not-allowed">
                                Next
                                <svg class="w-4 h-4 ml-1" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7" />
                                </svg>
                            </span>
                        <% } %>
                    </nav>
                </div>
                <% } %>
            </div>
        </div>

    </div>
</div>
